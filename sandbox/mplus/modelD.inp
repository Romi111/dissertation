TITLE: iris LPA

DATA:
    File is iris.dat
    
VARIABLE: 
    Names are Sepal_Length Sepal_Width Petal_Length Petal_Width;
    Classes = c(2) ;
            
MODEL:
    %overall%
    [Sepal_Length Sepal_Width Petal_Length Petal_Width];
    Sepal_Length Sepal_Width Petal_Length Petal_Width; 
    Sepal_Length WITH Sepal_Width Petal_Length Petal_Width;
    Sepal_Width WITH Petal_Length Petal_Width;
    Petal_Length WITH Petal_Width;

    %c#1%
    [Sepal_Length Sepal_Width Petal_Length Petal_Width];
    Sepal_Length Sepal_Width Petal_Length Petal_Width; 
    Sepal_Length WITH Sepal_Width Petal_Length Petal_Width(1-3);
    Sepal_Width WITH Petal_Length Petal_Width(4-5);
    Petal_Length WITH Petal_Width(6);

    %c#2%
    [Sepal_Length Sepal_Width Petal_Length Petal_Width];
    Sepal_Length Sepal_Width Petal_Length Petal_Width; 
    Sepal_Length WITH Sepal_Width Petal_Length Petal_Width(1-3);
    Sepal_Width WITH Petal_Length Petal_Width(4-5);
    Petal_Length WITH Petal_Width(6);
    
ANALYSIS: 
    Type is mixture;
            
OUTPUT:
    TECH1 TECH8 TECH11;